# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/unity/Hello_World/[Ll]ibrary/
/unity/Hello_World/[Tt]emp/
/unity/Hello_World/[[Oo]bj/
/unity/Hello_World/[Bb]uild/
/unity/Hello_World/[Bb]uilds/
/unity/Hello_World/[Ll]ogs/
/unity/Hello_World/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/unity/Hello_World/[Mm]emoryCaptures/

# Recordings can get excessive in size
/unity/Hello_World/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/unity/Hello_World/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/unity/Hello_World/.vs/

# Gradle cache directory
/unity/Hello_World/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/unity/Hello_World/ExportedObj/
/unity/Hello_World/.consulo/
*.csproj
*.unityproj
*.suo
*.tmp
*.user
*.userprefs
*.pidb
*.booproj
*.svd
*.pdb
*.mdb
*.opendb
*.VC.db
*.ipch

# Unity3D generated meta files
*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
/unity/Hello_World/sysinfo.txt

# Builds
*.apk
*.aab
*.unitypackage
*.app

# Crashlytics generated file
/unity/Hello_World/crashlytics-build.properties

# Packed Addressables
/unity/Hello_World/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/unity/Hello_World/[Aa]ssets/[Ss]treamingAssets/aa.meta
/unity/Hello_World/[Aa]ssets/[Ss]treamingAssets/aa/*

# ROS build logs
/ros/bags/
/ros/cpp/build/
/ros/cpp/build/cpp_pubsub/
/ros/cpp/log/
/ros/cpp/install/lib/cpp_pubsub/
/ros/cpp/install/setup.bat
/ros/cpp/install/setup.ps1
/ros/cpp/install/share/ament_index/resource_index/parent_prefix_path/cpp_pubsub
/ros/python/build/
/ros/python/log/
/ros/python/install/
